            <PasswordField layoutX="239.0" layoutY="167.0" promptText="Digite sua senha" fx:id="txSenha"/>
        <Label alignment="CENTER" layoutX="297.0" layoutY="92.0" style="&#10;" text="Login" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <TextField layoutX="239.0" layoutY="132.0" promptText="Digite seu login" fx:id="txLogin"/>
            <Button layoutX="266.0" layoutY="200.0" mnemonicParsing="false" text="Entrar" fx:id="btEntrar"/>
            <Button layoutX="327.0" layoutY="200.0" mnemonicParsing="false" text="Fechar" fx:id="btSair">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Button>
      </children>
    </AnchorPane>
  </children>
